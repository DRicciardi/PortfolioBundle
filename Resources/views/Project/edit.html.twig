{% extends 'PortfolioBundle::layout.html.twig' %}

{% block content %}
    <h4>Edit project "{{ project.name }}"</h4>

    <form action="#" method="post" enctype="multipart/form-data">
        <fieldset>
            {{ form_widget(form) }}
            <input type="submit" value="Save" />
        </fieldset>
    </form>
{% endblock %}